<header>
	<div id="headWrap" class="clearfix">
		<div id="headLogo">
			<%= link_to image_tag("foodtruck-logo.png", class: "head-icon-image", alt: "Foodtruck Maps Logo"), '/', class: "head-icon-link", method: :get %>
		</div>
		<div id="navWrap">
			<nav>
				<ul id="navList">
					<!-- <li id="searchList"><input id="search" type="text" autocomplete="off" placeholder="Find" /></li> -->
					<li><i class="fa fa-home" aria-hidden="true"></i> <%= link_to "Home", root_path, method: :get, class: "homeLink"%></li>
					<% if logged_in? %>
						<li><i class="fa fa-user" aria-hidden="true"></i> <%= link_to "Profile", user_path(@current_user) %></li>
						<li><i class="fa fa-sign-out" aria-hidden="true"></i> <%= link_to "Log Out", logout_path, method: :delete, class: "logoutLink" %></li>
					<% else %>
						<li><i class="fa fa-user-plus" aria-hidden="true"></i> <%= link_to "Sign Up", signup_path, class: "signinLink" %></li>
						<li><i class="fa fa-sign-in" aria-hidden="true"></i> <%= link_to "Log In", login_path, class: "loginLink" %></li>
					<% end %>
				</ul>
			</nav>
		</div>
	</div>
</header>
