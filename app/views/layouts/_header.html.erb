<header>
	<div id="headWrap" class="clearfix">
		<div id="headLogo">
			<%= link_to image_tag("beermap-logo.png", alt: "Foodtruck Maps Logo"), '/', method: :get %>
		</div>
		<div id="navWrap">
			<nav>
				<ul id="navList">
					<li id="searchList"><input id="search" type="text" autocomplete="off" placeholder="Find" /></li>
					<li><%= link_to "Home", root_path, method: :get, class: "homeLink"%></li>
					<% if logged_in? %>
						<li><%= link_to "Profile", user_path(@current_user) %></li>
						<li><%= link_to "Log Out", logout_path, method: :delete, class: "logoutLink" %></li>
					<% else %>
						<li><%= link_to "Sign Up", signup_path, class: "signinLink" %></li>
						<li><%= link_to "Log In", login_path, class: "loginLink" %></li>
					<% end %>
				</ul>
			</nav>
		</div>
	</div>
</header>